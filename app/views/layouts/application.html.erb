<!DOCTYPE html>
<html>
<head>
  <title>Contacts</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container_16">
		<div id="header" class="grid_14 prefix_1 suffix_1">
			<%= render 'layouts/header' %>
		</div>
		<div class="clear"></div>
		<div id="flash_error" class="grid_14 prefix_1 suffix_1">	
			<%= (render 'layouts/error', {:error => flash[:error]}) if flash[:error] %>
		</div>
		<div class="clear"></div>
		<div id="main"  class="grid_14 prefix_1 suffix_1">
			<%= yield %>
		</div>
		<div class="clear"></div>			
		<div id="footer" class="grid_14 prefix_1 suffix_1">
			<%= render 'layouts/footer' %>
		</div>
		<div class="clear"></div>
	</div>
	

</body>
</html>



