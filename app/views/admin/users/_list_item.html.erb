<tr class=<%= cycle("odd", "even") %> >
	<td class="grid_3">
		<%= user.login %>
	</td>
	
	<td class="grid_5 alpha">
		<%= user.email %>
	</td>

	<td class="grid_1">
		<%= user.active == 1 ? "Yes" : "No" %>
	</td>
	<td class="grid_3 ">
		<%= user.active_until %>
	</td>
	<td class="grid_2 omega">
		<%= link_to("edit", edit_admin_user_path(user) ) %>
	</td>
	
</tr >	